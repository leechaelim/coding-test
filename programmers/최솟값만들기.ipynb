   "source": [
    "# 한 5분\n",
    "# 이 접근 맞나? 싶어서 한번 제출해봤는데 그냥 맞았다,,, \n",
    "# 정확히 알고 푼 건 아닌듯\n",
    "def solution(A,B):\n",
    "    answer = 0\n",
    "\n",
    "    A.sort()\n",
    "    B.sort(reverse = True)\n",
    "    \n",
    "    for i in range(len(A)):\n",
    "        answer += A[i]*B[i]\n",
    "    return answer"
   ]
  }
